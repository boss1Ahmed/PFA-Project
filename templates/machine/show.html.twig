<!DOCTYPE html>
<head>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>
<title>Machine</title>

{% block body %}
    <h1>Machine</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ machine.id }}</td>
            </tr>
            <tr>
                <th>Nom_machine</th>
                <td>{{ machine.nomMachine }}</td>
            </tr>
            <tr>
                <th>Date_install</th>
                <td>{{ machine.dateInstall ? machine.dateInstall|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DocumentName</th>
                <td>{{ machine.documentName }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_machine_index') }}">back to list</a>

    <a href="{{ path('app_machine_edit', {'id': machine.id}) }}">edit</a>

    {{ include('machine/_delete_form.html.twig') }}
{% endblock %}
